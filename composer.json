{
  "name": "sebrogala/xsv-doctrine",
  "description": "Middleware for Doctrine 2 integration with Zend Expressive",
  "type": "library",
  "homepage": "https://bitbucket.org/SebRogala/xsvdoctrine",
  "license": "proprietary",
  "authors": [
    {
      "name": "Sebastian Rogala",
      "homepage": "http://sebrogala.com/"
    }
  ],
  "require": {
    "zendframework/zend-servicemanager": "^2.5"
  },
  "require-dev": {
    "composer/composer": ">=1.0.0-alpha10",
    "phpunit/phpunit": "~5.0"
  },
  "autoload": {
    "psr-4": {
      "Xsv\\Doctrine\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "XsvTest\\Doctrine\\": "test/"
    }
  },
  "scripts": {
    "test": [
      "php vendor/squizlabs/php_codesniffer/scripts/phpcs",
      "php vendor/phpunit/phpunit/phpunit"
    ]
  }
}
